{
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "importOrder": [
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^[.]",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^[.]"
  ],
  "importOrderParserPlugins": ["importAssertions", "typescript", "jsx"],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
