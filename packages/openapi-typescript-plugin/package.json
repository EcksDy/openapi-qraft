{
  "name": "@openapi-qraft/openapi-typescript-plugin",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.0.2",
  "type": "module",
  "scripts": {
    "build": "rimraf dist/ && tsc --project tsconfig.build.json",
    "dev": "yarn build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint"
  },
  "dependencies": {
    "@openapi-qraft/plugin": "workspace:^",
    "commander": "^11.1.0",
    "openapi-typescript": "7.0.0-next.7"
  },
  "devDependencies": {
    "@openapi-qraft/eslint-config": "workspace:~",
    "@types/node": "^18.19.4",
    "eslint": "^9.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": false
    }
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!src/**/*.test.*",
    "!src/**/*.spec.*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  }
}
