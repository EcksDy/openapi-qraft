{
  "name": "@radist2s/qraft",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "yarn build --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "generate": "qraft --clean -i ../../openapi.json -o src/mocks/fixtures/api --schema-types-path '../../../../../../../openapi.d.ts'"
  },
  "type": "module",
  "devDependencies": {
    "@radist2s/qraft-cli": "workspace:~",
    "@tanstack/react-query": "^5.17.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.47",
    "@vitejs/plugin-react": "latest",
    "@vitest/coverage-v8": "^1.1.3",
    "jsdom": "latest",
    "msw": "^2.0.12",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "vitest": "^1.1.3"
  },
  "peerDependencies": {
    "@tanstack/query-core": "^4.0.0 || ^5.0.0",
    "@tanstack/react-query": "^4.0.0 || ^5.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/query-core": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": false
    },
    "react": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  }
}
