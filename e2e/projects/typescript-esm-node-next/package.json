{
  "name": "typescript-esm-node-next",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "generate-qraft": "openapi-qraft --plugin tanstack-query-react --plugin openapi-typescript https://raw.githubusercontent.com/swagger-api/swagger-petstore/7767363b841961221a38c0be9c6b1066a5120051/src/main/resources/openapi.yaml --clean -o src/api --openapi-types-import-path '../schema.d.ts' --explicit-import-extensions --openapi-types-file-name schema.d.ts",
    "e2e:pre-build": "npm run generate-qraft",
    "e2e:post-build": " node ./dist/index.js && node ./dist/commonjs-client.cjs"
  },
  "dependencies": {
    "typescript": "latest",
    "@openapi-qraft/cli": "1.11.0-beta.2",
    "@openapi-qraft/react": "1.11.0-beta.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenAPI-Qraft/openapi-qraft.git"
  }
}
