{
  "name": "playground",
  "private": true,
  "version": "0.0.14",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "generate-types": "openapi-typescript https://petstore3.swagger.io/api/v3/openapi.json -o ./schema.d.ts",
    "generate-qraft": "openapi-qraft https://petstore3.swagger.io/api/v3/openapi.json --clean -o src/api --openapi-types-import-path '../../../schema.d.ts'",
    "e2e:pre-build": "npm run generate-types && npm run generate-qraft"
  },
  "dependencies": {
    "@openapi-qraft/cli": "workspace:~",
    "@openapi-qraft/react": "workspace:~",
    "constate": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "openapi-typescript": "latest",
    "typescript": "^5.2.2",
    "vite": "^5.0.11"
  }
}
